{
  "name": "foe-tools",
  "version": "2.69.1",
  "description": "Set of tools for Forge of Empires",
  "main": "nuxt.config.js",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:prod": "cross-env NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
    "build:gh": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "release": "standard-version",
    "deploy": "push-dir --dir=dist --branch=master --cleanup",
    "coverage": "codecov",
    "contributors:add": "all-contributors --config all-contributorsrc.json add",
    "contributors:generate": "all-contributors --config all-contributorsrc.json generate",
    "test:lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "test:e2e": "testcafe firefox test/e2e/ --app \"cross-env PORT=8080 npm run dev\" --app-init-delay 35000",
    "test:unit": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=false",
    "test:unit:coverage": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=true",
    "test:unit:watch": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=false --testRegex ^.*.test.js$ test/unit/ --watch",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 npm run test:unit",
    "update-foe-data": "node .scripts/update-gb-data.js",
    "i18n:generate:all": "vuei18n-po --es6 --pluralRules=plugins/vue-i18n-plural.js --messagesDir=lang translations/*.po",
    "i18n:generate": "vuei18n-po --messagesDir=lang translations/*.po"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FOE-Tools/FOE-Tools.github.io.git"
  },
  "keywords": [
    "foe",
    "forge",
    "empire",
    "nuxt",
    "vue",
    "yarn",
    "tool",
    "forge-of-empires",
    "investment"
  ],
  "author": "Heziode <Heziode@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FOE-Tools/FOE-Tools.github.io/issues"
  },
  "homepage": "https://foe-tools.github.io/",
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime-corejs3": "^7.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@nuxt/babel-preset-app": "^2.14.4",
    "@nuxtjs/router-extras": "^1.1.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@vue/test-utils": "^1.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "core-js": "3",
    "cross-env": "^7.0.2",
    "csv-parser": "^2.3.3",
    "download": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.4.3",
    "jest-serializer-vue": "^2.0.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "lodash.values": "^4.3.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.0.1",
    "standard-version": "^9.0.0",
    "testcafe": "^1.9.1",
    "testcafe-nuxt-selectors": "^1.0.6",
    "uuid": "^8.3.0",
    "vue-jest": "^3.0.6",
    "vuei18n-po": "^1.3.0"
  },
  "dependencies": {
    "@hcaptcha/vue-hcaptcha": "^0.2.2",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.2",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sentry": "^4.3.4",
    "@panter/vue-i18next": "^0.15.2",
    "all-contributors-cli": "^6.17.2",
    "buefy": "^0.9.2",
    "bulma": "^0.9.0",
    "bulmaswatch": "^0.8.1",
    "chart.js": "^2.9.3",
    "codecov": "^3.7.2",
    "cookie-universal-nuxt": "^2.1.4",
    "country-flag-emoji": "^1.0.3",
    "dayjs": "^1.8.34",
    "enumify": "^1.0.4",
    "facebook-locales": "^1.0.916",
    "get-user-locale": "^1.4.0",
    "js-priority-queue": "^0.1.5",
    "jsdom": "^16.4.0",
    "nuxt": "^2.14.4",
    "nuxt-i18n": "^6.13.12",
    "orejime": "^1.2.3",
    "remark": "^12.0.1",
    "remark-emoji": "^2.1.0",
    "remark-html": "^12.0.0",
    "shepherd.js": "^7.2.1",
    "vue-clipboards": "^1.3.0",
    "vue-content-loader": "^0.2.3",
    "vue-router": "^3.4.3",
    "vuex-pathify": "^1.4.1",
    "vuex-persistedstate": "^3.1.0"
  }
}
